    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "prettier": "^2.8.8",
    "prettier-plugin-css-order": "^1.3.1",
    "prettier-plugin-multiline-arrays": "^1.1.4"
  },
  "peerDependencies": {
    "@roots/bud-eslint": "^6.17.0",
    "@roots/eslint-config": "^6.16.1",
    "eslint": ">=7.32.0 <9.0.0",
